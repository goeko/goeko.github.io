javascript:(function(){var outputText="";document.querySelectorAll("[itemprop='containsSeason']").forEach(function(e){e.querySelectorAll("a").forEach(function(e){let t=e.querySelector('[itemprop="name"]:first-child');if(t){let n=document.querySelector('[data-event-category="serientitel"]'),o=e.querySelector('.episodenliste-schmal');if(n&&o){let l="S0"+o.innerText.trim().replace('.', 'E');outputText+=n.innerText.trim()+" "+l+" - "+t.innerText.trim()+"\n"}}}),outputText+="\n"}),navigator.clipboard.writeText(outputText).then(()=>{var e="Text in den Zwischenspeicher kopiert.";console.log(e),alert(e)}).catch(e=>{console.error("Fehler beim Kopieren in den Zwischenspeicher: ",e)})})();